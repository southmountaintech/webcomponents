{
  "hosting": {
    "public": "public",
    "site": "cdn-smountaintech",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [{
      "source": "**/*.@(js)",
      "headers": [{
        "key": "Access-Control-Allow-Origin",
        "value": "*"
      },{
        "Cache-Control": "Access-Control-Allow-Origin",
        "value": "max-age=604800, public"
      }]
    }]
  }
}
